ck=\"removeFavourite(index)\">‚úñÔ∏è</button>\r\n                            </p>\r\n                        </div>\r\n                    </div>\r\n\r\n\r\n\r\n\r\n                </div>\r\n            </div>\r\n            <div class=\"button\" id=\"map\" style=\"align-items: center; margin-top: 1%\">\r\n                <GMapMap :center=\"center\" :zoom=\"12\" map-type-id=\"terrain\" style=\"width: 100%; height: 700px\"\r\n                    ref=\"mapTheme\">\r\n                    <GMapMarker v-for=\"(marker, index) in stops.stops\" :key=\"marker\"\r\n                        :position=\"{ lat: stops.stops[index].latitude, lng: stops.stops[index].longitude }\"\r\n                        :visible=\"marker.visibility\" :title=\"marker.searchname\" :clickable=\"true\" :icon='{\r\n                            url: \"https://img.icons8.com/fluency/48/000000/bus.png\",\r\n                            scaledSize: { width: 40, height: 40 }\r\n                        }' @click=\"openMarker(stops.stops[index].stopid); realTimeBusData(stops.stops[index].stopid)\">\r\n\r\n                        <GMapInfoWindow :closeclick=\"true\" @closeclick=\"openMarker(null)\"\r\n                            :opened=\"openedMarkerID === stops.stops[index].stopid\">\r\n                            <div style=\"text-align: center;  height:200px; overflow:auto\">\r\n                                <h5>Real Time Information</h5>\r\n                                <div>Stop Name: {{ stops.stops[index].searchname }}</div>\r\n                                <div>\r\n                                    <div v-if=\"loading\" style=\"margin: 0 auto;\">\r\n                                        <div class=\"loader\" style=\"margin: 0 auto;\"></div>\r\n                                        <div>Loading Real Time Data</div>\r\n                                    </div>\r\n                                    <div v-else>\r\n\r\n                                        <table id=\"realTimeTable\" style=\"margin: 0 auto;\">\r\n                                            <tr>\r\n                                                <th>Bus Route</th>\r\n                                                <th>Destination</th>\r\n                                                <th>Arrival</th>\r\n\r\n                                            </tr>\r\n\r\n                                            <tr v-for=\"busInfo in resultBusTimesSched\" :key=\"busInfo\">\r\n                                                <td>{{ busInfo.Route }}</td>\r\n                                                <td>{{ busInfo.Destination }}</td>\r\n                                                <td>{{ busInfo.Arrival }}</td>\r\n\r\n                                            </tr>\r\n                                        </table>\r\n                                    </div>\r\n                                </div>\r\n\r\n                            </div>\r\n                        </GMapInfoWindow>\r\n                    </GMapMarker>\r\n                </GMapMap>\r\n            </div>\r\n\r\n        </div>\r\n    </ContentBase>\r\n</template>\r\n\r\n<script>\r\nimport ContentBase from '../components/ContentBase.vue'\r\nimport busRoutesJson from \"../assets/json/route.json\";\r\n\r\n\r\n// import VueGoogleAutocomplete from \"vue-google-autocomplete\"\r\nimport { ref } from 'vue';\r\nimport $ from 'jquery';\r\n\r\nconst newFavourite = ref('')\r\n\r\nexport default {\r\n    name: \"RouteStopInfoView\",\r\n    components: {\r\n        ContentBase,\r\n    },\r\n\r\n    data() {\r\n        return {\r\n            openedMarkerID: null,\r\n            currentLocation: null,\r\n            busRoutes: busRoutesJson,\r\n            realTimeResults: null,\r\n            resultBusTimesSched: {},\r\n            loading: false,\r\n            favourites: [],\r\n            newFavourite: null,\r\n        }\r\n    },\r\n\r\n\r\n    setup() {\r\n        let stops = ref([]);\r\n        let routeId = ref('');\r\n        // let newFavourite = ref('')\r\n        const submit = () => {\r\n            $.ajax({\r\n                url: \"http://127.0.0.1:9000/getinfo\",\r\n                type: \"GET\",\r\n                data: {\r\n                    routeId: routeId.value,\r\n                },\r\n                success(resp) {\r\n                    stops.value = resp;\r\n                    console.log(stops.value)\r\n                }\r\n            });\r\n        };\r\n\r\n        return {\r\n            stops,\r\n            routeId,\r\n            // newFavourite,\r\n            submit,\r\n            // submit2,\r\n            center: {\r\n                lat: 53.349722,\r\n                lng: -6.260278,\r\n            },\r\n        }\r\n    },\r\n    mounted() {\r\n        if (localStorage.getItem('favourites')) {\r\n            try {\r\n                this.favourites = JSON.parse(localStorage.getItem('favourites'));\r\n            } catch (e) {\r\n                localStorage.removeItem('favourites');\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        openMarker(id) {\r\n            this.openedMarkerID = id;\r\n        },\r\n        realTimeBusData(busstopNO) {\r\n            this.loading = true\r\n            fetch('http://127.0.0.1:9000/getRealTime/' + busstopNO)\r\n                .then(response => response.json())\r\n                .then(data => this.resultBusTimesSched = data)\r\n                .finally(() => (this.loading = false))\r\n\r\n\r\n        },\r\n        addFavourite() {\r\n            if (!this.newFavourite) {\r\n                return;\r\n            }\r\n\r\n            this.favourites.push(this.newFavourite);\r\n            this.newFavourite = '';\r\n            this.saveFavourite();\r\n            // console.log(\"add favourite complete\")\r\n        },\r\n        removeFavourite(x) {\r\n            this.favourites.splice(x, 1);\r\n            this.saveFavourite();\r\n            // console.log(\"remove favourite complete\")\r\n        },\r\n        saveFavourite() {\r\n            const parsed = JSON.stringify(this.favourites);\r\n            localStorage.setItem('favourites', parsed);\r\n            // console.log(\"save favourite complete\")\r\n        },\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#realTimeTable tr:nth-child(even) {\r\n    background-color: #f2f2f2;\r\n}\r\n\r\n#realTimeTable tr:hover {\r\n    background-color: #ddd;\r\n}\r\n\r\n#realTimeTable th {\r\n    background-color: #009B77;\r\n}\r\n\r\n#realTimeTable td,\r\n#realTimeTable th {\r\n    border: 1px solid #ddd;\r\n}\r\n\r\n.loader {\r\n    border: 16px solid #f3f3f3;\r\n    border-radius: 50%;\r\n    border-top: 16px solid #3498db;\r\n    width: 30px;\r\n    height: 30px;\r\n    -webkit-animation: spin 2s linear infinite;\r\n    /* Safari */\r\n    animation: spin 2s linear infinite;\r\n}\r\n\r\n/* Safari */\r\n@-webkit-keyframes spin {\r\n    0% {\r\n        -webkit-transform: rotate(0deg);\r\n    }\r\n\r\n    100% {\r\n        -webkit-transform: rotate(360deg);\r\n    }\r\n}\r\n\r\n@keyframes spin {\r\n    0% {\r\n        transform: rotate(0deg);\r\n    }\r\n\r\n    100% {\r\n        transform: rotate(360deg);\r\n    }\r\n}\r\n</style>","C:\\Users\\Johnson\\Desktop\\dublinBus\\dbus\\src\\views\\UserLocationView.vue",[],"C:\\Users\\Johnson\\Desktop\\dublinBus\\dbus\\src\\views\\WeatherView.vue",[],"C:\\Users\\Johnson\\Desktop\\dublinBus\\dbus\\src\\components\\ContentBase.vue",["87"],"C:\\Users\\Johnson\\Desktop\\dublinBus\\dbus\\src\\components\\MapDisplay.vue",[],"C:\\Users\\Johnson\\Desktop\\dublinBus\\dbus\\src\\components\\UserLocation.vue",[],"C:\\Users\\Johnson\\Desktop\\dublinBus\\dbus\\src\\components\\WeatherDisplay.vue",[],{"ruleId":"88","severity":1,"message":"89","line":116,"column":7,"nodeType":"90","messageId":"91","endLine":116,"endColumn":19},{"ruleId":"88","severity":1,"message":"92","line":53,"column":5,"nodeType":"90","messageId":"91","endLine":53,"endColumn":17},"@typescript-es